{
  "version": "2.0",
  "extensions": {
    "queues": {
      "maxPollingInterval": 2000,
      "visibilityTimeout": "00:00:30",
      "batchSize": 16,
      "maxDequeueCount":10,
      "newBatchThreshold": 8
    }
  },
    "logging": {
      "applicationInsights": {
        "samplingSettings": {
          "isEnabled": true,
          "excludedTypes": "Request"
        },
        "enableLiveMetricsFilters": true
      }
    }
  }